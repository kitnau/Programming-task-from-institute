#include <iostream>
using namespace std;

int F1 (int n, int &count1){
    int f[n];
    int i;
    
    f[1] = 1;
    f[2] = 3;
    count1 ++;
    for (i = 3; i <= n; i++)
    {
        f[i] = f [i-1] + f[i-2] + (i - 2);
        count1++;
    }
    return f[n];
}

int F2 (int n, int &count2){
    if (n == 1){
        count2 ++;
        return 1;
    }
    else if (n == 2){
        count2 ++;
        return 3;
    }
    count2 ++;
    return F2(n-1, count2) + F2(n-2, count2) + (n-2);
}

int main ()
{
    int c1;
    int count1 = 0;
    int count2 = 0;
    cin >> c1;

    cout << F1(c1, count1) << " , повторений оптимизированным способом: " << count1 << "\n";
    cout << F2(c1, count2) << " , повторений НЕоптимизированным способом: " << count2 << "\n";
    return 0;
    
}
