#include <iostream>
#include <string>
using namespace std;



string check_bracket(char inner_symbol, int &n, long *massiv, int &leng){
    if (inner_symbol ==  '('){
        n++;
        massiv[n] = 1;
    } 
    else if (inner_symbol ==  '[') {
        n++;
        massiv[n] = 2;
    }
    else if (inner_symbol ==  '{') { 
        n++;
        massiv[n] = 3;
    }
    else if (inner_symbol ==  ')') {
        for (int g = n; g > -2; g--){
            if (g == -1) return "NO";
            if (massiv[g] == 2 || massiv[g] == 3){
                return "NO";
            }
            else {
                if (massiv[g] == 1){
                    massiv[g] = -1;
                    n--;
                    return "YES";
                }
            }
        }
    }
    else if (inner_symbol ==  ']') {
        for (int g = n; g > -2; g--){
            if (g == -1) return "NO";
            if (massiv[g] == 1 || massiv[g] == 3){
                return "NO";
            }
            else {
                if (massiv[g] == 2){
                    massiv[g] = -1;
                    n--;
                    return "YES";
                }
            }
        }
    }
    else if (inner_symbol ==  '}') {
        for (int g = n; g > -2; g--){
            if (g == -1) return "NO";
            if (massiv[g] == 1 || massiv[g] == 2){
                return "NO";
            }
            else {
                if (massiv[g] == 3){
                    massiv[g] = -1;
                    n--;
                    return "YES";
                }
                
            }
        }
    
    }
    return "YES";
}



int main()
{
    string stroka;
    int flag1 = 0; // отвечает за проверку скобок ()
    int flag2 = 0; // отвечает за проверку скобок []
    int flag3 = 0; // отвечает за проверку скобок {}
    int n = -1;
    string flag_error = "YES";
    cin >> stroka;
    int leng = stroka.size();
    long massiv[leng];

    
    for (int i = 0; i < leng; i++) {
        if (check_bracket (stroka[i], n, massiv, leng) == "NO") {
            flag_error = "NO";
            break;
        }
        }
    cout << flag_error;
    return 0;
}
