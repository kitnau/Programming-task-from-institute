#include <iostream>
#include <string>
using namespace std;



string check_bracket(char inner_symbol, int &flag1, int &flag2, int &flag3, int &n, int *massiv){
    n++;
    if (inner_symbol ==  '('){
        flag1++;
        massiv[n] = 1;
    } 
    else if (inner_symbol ==  '[') {
        flag2++;
        massiv[n] = 2;
    }
    else if (inner_symbol ==  '{') { 
        flag3++;
        massiv[n] = 3;
    }
    if (inner_symbol ==  ')') {
        if (flag1 == 0) return "NO";
        flag1 = flag1-1;
        
        for (int i = (sizeof (massiv) /4); i > -1; i--){
            if (massiv[i] == 2 || massiv[i] == 3){
                return "NO";
            }
            else {
                if (massiv[i] == 1){
                    massiv[i] = -1;
                    return "YES";
                }
            }
        }
    }
    else if (inner_symbol ==  ']') {
        if (flag2 == 0) return "NO";
        flag2 = flag2-1;
        for (int i = (sizeof (massiv) /4); i > -1; i--){
            if (massiv[i] == 1 || massiv[i] == 3){
                return "NO";
            }
            else {
                if (massiv[i] == 2){
                    massiv[i] = -1;
                    return "YES";
                }
            }
        }
    }
    else if (inner_symbol ==  '}') {
        if (flag3 == 0) return "NO";
        flag3 = flag3-1;
        for (int i = (sizeof (massiv) /4); i > -1; i--){
            if (massiv[i] == 1 || massiv[i] == 2){
                return "NO";
            }
            else {
                if (massiv[i] == 3){
                    massiv[i] = -1;
                    return "YES";
                }
            }
        }
    }
    return "YES";
}



int main()
{
    string stroka;
    int flag1 = 0; // отвечает за проверку скобок ()
    int flag2 = 0; // отвечает за проверку скобок []
    int flag3 = 0; // отвечает за проверку скобок {}
    int n = -1;
    string flag_error = "YES";
    cin >> stroka;
    int massiv[stroka.size()/2];
    for (int i = 0; i < stroka.size()/2; i++) massiv[i]  = -2;

    for (int i = 0; i < stroka.size(); i++) {
        if (check_bracket (stroka[i], flag1, flag2, flag3, n, massiv) == "NO") {
            flag_error = "NO";
            break;
        }
        }
    if ((flag1 or flag2 or flag3) != 0 ) flag_error = "NO";
    cout << flag_error;
    return 0;
}
//[(()])
